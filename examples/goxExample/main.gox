package main

import (
	"github.com/cstevenson98/goFE/pkg/goFE"
	"github.com/cstevenson98/goFE/examples/goxExample/components"
)

func main() {
	goFE.Init(&goFE.Logger{
		Level: goFE.DEBUG,
	})

	goFE.SetDocument(goFE.NewDocument([]goFE.Component{
		components.Counter(&components.CounterProps{
			InitialCount: 0,
		}),
	}))

	goFE.GetDocument().Init()
	<-make(chan bool)
} 